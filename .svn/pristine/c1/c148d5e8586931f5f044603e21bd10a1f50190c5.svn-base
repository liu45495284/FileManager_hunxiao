    private Handler refreshFileListHandler = new Handler() {
        public void handleMessage(Message msg) {
            switch (msg.what) {
            case TYPE_NOTIFY_REFRESH_HIDEFILE:
                showUI();
                break;
            }
            super.handleMessage(msg);
        }

    };

