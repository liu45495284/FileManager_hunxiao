    <style name="updateself.AppTextAppearance.Notification.Content" parent="android:TextAppearance.StatusBar.EventContent" />
    <style name="updateself.AppTextAppearance.Notification.Title" parent="android:TextAppearance.StatusBar.EventContent.Title" /> 
